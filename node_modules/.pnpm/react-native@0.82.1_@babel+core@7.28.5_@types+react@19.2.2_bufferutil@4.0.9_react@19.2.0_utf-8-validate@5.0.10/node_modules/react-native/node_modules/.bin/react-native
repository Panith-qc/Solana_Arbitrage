#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/panith_069/Solana_Arbitrage/node_modules/.pnpm/react-native@0.82.1_@babel+core@7.28.5_@types+react@19.2.2_bufferutil@4.0.9_react@19.2.0_utf-8-validate@5.0.10/node_modules/react-native/node_modules:/home/panith_069/Solana_Arbitrage/node_modules/.pnpm/react-native@0.82.1_@babel+core@7.28.5_@types+react@19.2.2_bufferutil@4.0.9_react@19.2.0_utf-8-validate@5.0.10/node_modules:/home/panith_069/Solana_Arbitrage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/panith_069/Solana_Arbitrage/node_modules/.pnpm/react-native@0.82.1_@babel+core@7.28.5_@types+react@19.2.2_bufferutil@4.0.9_react@19.2.0_utf-8-validate@5.0.10/node_modules/react-native/node_modules:/home/panith_069/Solana_Arbitrage/node_modules/.pnpm/react-native@0.82.1_@babel+core@7.28.5_@types+react@19.2.2_bufferutil@4.0.9_react@19.2.0_utf-8-validate@5.0.10/node_modules:/home/panith_069/Solana_Arbitrage/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
