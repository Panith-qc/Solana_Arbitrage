#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_react@19.2.0/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../react-native@0.82.0_@babel+core@7.28.4_@types+react@19.2.2_react@19.2.0/node_modules/react-native/cli.js" "$@"
fi
