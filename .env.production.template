# PRODUCTION ENVIRONMENT TEMPLATE
# Copy this to .env.production and fill in your actual values
# NEVER commit the real .env.production file to git!

# Application
NODE_ENV=production
VITE_APP_ENV=production

# Solana Network
VITE_SOLANA_NETWORK=mainnet-beta
VITE_SOLANA_RPC_URL=YOUR_HELIUS_RPC_URL_HERE

# Helius MEV Service
VITE_HELIUS_API_KEY=YOUR_HELIUS_API_KEY_HERE
VITE_HELIUS_RPC_URL=YOUR_HELIUS_RPC_URL_HERE

# Supabase
VITE_SUPABASE_URL=YOUR_SUPABASE_URL_HERE
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY_HERE

# Jupiter API
VITE_JUPITER_API_URL=https://quote-api.jup.ag

# Trading Configuration - SAFE DEFAULTS
VITE_MIN_PROFIT_USD=0.05
VITE_MAX_SLIPPAGE_BPS=100
VITE_PRIORITY_FEE_LAMPORTS=10000
VITE_MAX_POSITION_SOL=1.0
VITE_AUTO_TRADING_ENABLED=false

# Cloud Run Configuration
PORT=8080

# Security
VITE_ENABLE_ANALYTICS=false
VITE_LOG_LEVEL=info

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW_MS=60000
